  keywords = {Evapotranspiration (ET), Evaporation, Ensemble modelling, R package, Evapotranspiration software},
  language = {en},
  url      = {https://www.sciencedirect.com/science/article/pii/S136481521530133X},
  urldate  = {2022-06-17},
}

@InCollection{Hansen2006,
