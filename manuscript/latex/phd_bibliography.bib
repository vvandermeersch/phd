  author     = {Cameron, David and Hartig, Florian and Minnuno, Francesco and Oberpriller, Johannes and Reineking, Björn and Van Oijen, Marcel and Dietze, Michael},
  journal    = {Methods in Ecology and Evolution},
  title      = {Issues in calibrating models with multiple unbalanced constraints: the significance of systematic model and data errors},
  year       = {2022},
