  groups   = {Model skill},
  keywords = {High-resolution, Projections, CMIP6, ERA5-Land, Downscaling, Climate change, Adaptation, Impact modeling},
  url      = {https://www.sciencedirect.com/science/article/pii/S2352340922008745},
