@Article{Oreskes1994,
  author    = {Oreskes, Naomi and Shrader-Frechette, Kristin and Belitz, Kenneth},
  journal   = {Science},
  title     = {Verification, {Validation}, and {Confirmation} of {Numerical} {Models} in the {Earth} {Sciences}},
  year      = {1994},
  month     = feb,
  number    = {5147},
  pages     = {641--646},
  volume    = {263},
  abstract  = {Verification and validation of numerical models of natural systems is impossible. This is because natural systems are never closed and because model results are always nonunique. Models can be confirmed by the demonstration of agreement between observation and prediction, but confirmation is inherently partial. Complete confirmation is logically precluded by the fallacy of affirming the consequent and by incomplete access to natural phenomena. Models can only be evaluated in relative terms, and their predictive value is always open to question. The primary value of models is heuristic.},
  doi       = {10.1126/science.263.5147.641},
  file      = {Full Text PDF:https\://www.science.org/doi/pdf/10.1126/science.263.5147.641:application/pdf},
  groups    = {Discussion},
  publisher = {American Association for the Advancement of Science},
  url       = {https://www.science.org/doi/10.1126/science.263.5147.641},
  urldate   = {2023-09-22},
}

@Article{Higgins2012,
  author     = {Higgins, Steven I. and O'Hara, Robert B. and Römermann, Christine},
  journal    = {Journal of Biogeography},
  title      = {A niche for biology in species distribution models},
  year       = {2012},
  issn       = {1365-2699},
  number     = {12},
  pages      = {2091--2095},
  volume     = {39},
  abstract   = {Why species are found where they are is a central question in biogeography. The most widely used tool for understanding the controls on distribution is species distribution modelling. Species distribution modelling is now a well-established method in both the theoretical and applied ecological literature. In this special issue we examine the current state of the art in species distribution modelling and explore avenues for including more biological processes in such models. In particular we focus on physiological, demographic, dispersal, competitive and ecological-modulation processes. This overview highlights opportunities for new species distribution model concepts and developments, as well as a statistical agenda for implementing such models.},
  doi        = {10.1111/jbi.12029},
  file       = {:model_comparison/Higgins2012 - A Niche for Biology in Species Distribution Models.pdf:PDF},
  groups     = {model_comparison, correlative_models, mechanistic_models, guidelines, Read or skimmed, Introduction},
  keywords   = {Biodiversity, correlative models, model fitting, niche, process-based models, species distributions},
  language   = {en},
